{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "group:all",
        ":dependencyDashboard",
        "schedule:daily"
    ],
    "baseBranches": [
        "develop"
    ],
    "packageRules": [
        {
            "matchUpdateTypes": ["major"],
            "labels": ["UPDATE-MAJOR"]
        },
        {
            "matchPackageNames": [
                "com.google.protobuf"
            ],
            "allowedVersions": "<=0.8.19"
        }
    ]
}
