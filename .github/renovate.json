{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "group:all",
        ":dependencyDashboard",
        "schedule:daily"
    ],
    "baseBranches": [
        "develop"
    ],
    "packageRules": [
        {
            "matchAdoptionLevel": ">=80%",
            "commitMessagePrefix": "[AUTO]",
            "commitMessageTopic": "{{depName}}",
            "commitMessage": "Update to {{#if isFromDependencyFile}}{{oldVersion}}{{else}}{{newVersion}}{{/if}}"
        },
        {
            "matchPackageNames": [
                "com.google.protobuf"
            ],
            "allowedVersions": "<=0.8.19"
        }
    ]
}
