{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "group:all",
        ":dependencyDashboard",
        "schedule:daily"
    ],
    "baseBranches": [
        "develop"
    ],
    "packageRules": [
        {
            "matchUpdateTypes": ["minor", "patch"],
            "labels": ["UPDATE-minor-patch"]
        }
    ]
}
