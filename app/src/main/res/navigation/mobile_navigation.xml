<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.keelim.cnubus.ui.Broot.BFragment"
        android:label="@string/a_root"
        tools:layout="@layout/fragment_aroot" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.keelim.cnubus.ui.Aroot.AFragment"
        android:label="@string/b_root"
        tools:layout="@layout/fragment_broot" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.keelim.cnubus.ui.Croot.CFragment"
        android:label="@string/c_root"
        tools:layout="@layout/fragment_croot" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.keelim.cnubus.ui.Setting.setting"
        android:label="setting"
        tools:layout="@layout/setting_fragment"/>
</navigation>