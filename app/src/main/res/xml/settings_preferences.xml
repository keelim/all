<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"
    app:key="total">
    <PreferenceCategory
        app:key="feature_category"
        app:title="관련 링크">
        <Preference
            app:key="blog"
            app:title="관련블로그" />

        <Preference
            app:key="home"
            app:title="홈페이지" />
    </PreferenceCategory>

    <PreferenceCategory app:title="기능">
        <Preference
            app:key="update"
            app:title="업데이트" />

        <Preference
            app:key="search"
            app:title="검색" />

        <ListPreference
            app:defaultValue="default"
            app:dialogTitle="Light Mode/Dark Mode"
            app:entries="@array/themeListArray"
            app:entryValues="@array/themeEntryArray"
            app:key="themePref"
            app:title="테마 선택하기"
            app:useSimpleSummaryProvider="true" />

    </PreferenceCategory>

    <PreferenceCategory
        app:key="other_category"
        app:title="약관">
        <Preference
            app:key="opensource"
            app:title="오픈소스 라이선스"></Preference>
    </PreferenceCategory>
</androidx.preference.PreferenceScreen>
